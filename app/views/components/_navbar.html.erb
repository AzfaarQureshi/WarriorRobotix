<% if member_signed_in? %>
<ul id="member_menu_dropdown" class="dropdown-content">
  <li class="text"><span><%= current_member.full_name %></span></li>
  <li><%= link_to "Members Center", members_path %></li>
  <li class="divider"></li>
  <li><%= link_to "Sign out", signout_path(return_to_info), method: :delete %></li>
</ul>
<% end %>
<ul id="competitions_dropdown" class="dropdown-content">
  <li class="text"><span>Competitions</span></li>
  <li><%= link_to "Upcoming & Past", competitions_path %></li>
  <li><%= link_to "Vex", vex_path %></li>
  <li><%= link_to "Skills", skills_path %></li>
  <li><%= link_to "FRC/CETA", frc_ceta_path %></li>
</ul>
<ul id="about_dropdown" class="dropdown-content">
  <li class="text"><span>About</span> </li>
  <li><%= link_to "Club", about_path %></li>
  <li><%= link_to "Teams", teams_path %></li>
  <li><%= link_to "Sponsors", sponsors_path %></li>
</ul>
<div class="navbar-fixed">
  <nav>
    <div class="nav-wrapper">
    <span class="brand-logo flow-text" style="font-size:132%">
    <%= link_to "WarriorRobotix", root_path %>
    </span>
    <ul class="flow-text right hide-on-med-and-down top-nav" style="font-size:100%;">
      <% if member_is_admin? %>
      <li>
        <%= link_to "Attendance", attendances_center_path, class: 'nav-attend' %>
      </li>
      <% elsif member_signed_in? && !member_is_admin? %>
      <li>
        <%= link_to "Attendance", '/myattendance', class: 'nav-attend' %>
      </li>
      <% end %>
      <li>
        <a class="dropdown-button nav-competitions" href="#!" data-activates="competitions_dropdown">
          Competitions <i class="material-icons right">keyboard_arrow_down</i>
        </a>
      </li>
      <li>
        <a class="dropdown-button nav-about" href="#!" data-activates="about_dropdown">
          About <i class="material-icons right">keyboard_arrow_down</i>
        </a>
      </li>
      <li>
        <% if member_signed_in? %>
        <%= link_to "Posts", posts_path, class: 'nav-posts' %>
        <% else %>
        <%= link_to "Blog", posts_path, class: 'nav-posts' %>
        <% end %>
      </li>
      <li>
        <%= link_to "Videos", videos_path, class: 'nav-videos' %>
      </li>
      <li>
        <% if member_signed_in? %>
        <a class="dropdown-button" href="#!" data-activates="member_menu_dropdown">
          <i class="material-icons left">
          <%= member_is_admin? ? 'people' : 'person' %>
          </i>
          <%= current_member.first_name %>
        </a>
        <% else %>
          <%= link_to signin_path do %>
            <i class="material-icons">person_outline</i>
          <% end %>
        <% end %>
      </li>
    </ul>
    <ul id="slide-out" class="side-nav flow-text">
      <% if member_signed_in? %>
      <li>
        <a>Hi, <%= current_member.full_name %></a>
      </li>
      <% end %>
      <li>
        <% if member_signed_in? %>
          <%= link_to "Sign out", signout_path(return_to_info), method: :delete %>
        <% else %>
          <%= link_to "Sign in", signin_path(return_to_info) %>
        <% end %>
      </li>
      <% if member_is_admin? %>
      <li>
        <%= link_to "Attendance", attendances_center_path %>
      </li>
      <% elsif member_signed_in? && !member_is_admin? %>
      <li>
        <%= link_to "Attendance", '/myattendance' %>
      </li>
      <% end %>
      <% if member_signed_in? %>
      <li>
        <%= link_to "Members Center", members_path %>
      </li>
      <% end %>
      <li>
        <%= link_to "Upcoming & Past", competitions_path %>
      </li>
      <li>
        <%= link_to "Vex", vex_path %>
      </li>
      <li>
        <%= link_to "Skills", skills_path %>
      </li>
      <li>
        <%= link_to "FRC/CETA", frc_ceta_path %>
      </li>
      <li>
        <%= link_to "About Club", competitions_path %>
      </li>
      <li>
        <%= link_to "Teams", teams_path %>
      </li>
      <li>
        <%= link_to "Sponsors", sponsors_path %>
      </li>
      <li>
        <%= link_to "Posts", posts_path %>
      </li>
      <li>
        <%= link_to "Videos", videos_path %>
      </li>
    </ul>
    <a href="#" data-activates="slide-out" class="button-collapse"><i class="mdi-navigation-menu"></i></a>
    </div>
  </nav>
</div>
